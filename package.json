{
  "name": "@deeplinknow/react-native",
  "version": "1.1.10",
  "description": "React Native + Expo SDK for the Deeplink Now deferred deep linking platform",
  "source": "./src/index.ts",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/index.d.ts",
  "typings": "./lib/typescript/commonjs/index.d.ts",
  "scripts": {
    "example": "yarn workspace react-native-deeplink-now-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "clean": "del-cli lib",
    "prepare": "bob build && eslint \"**/*.{js,ts,tsx}\" --fix",
    "release": "yarn clean && yarn lint:fix && yarn prepare && git add -A && git commit -m 'chore: prepare release' --no-verify && git push && yarn release-it",
    "release-it": "release-it"
  },
  "exports": {
    ".": {
      "import": "./lib/module/index.js",
      "require": "./lib/commonjs/index.js"
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android",
    "expo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeplinknow/dln-react-native.git"
  },
  "author": "DeepLinkNow <2121331+jvgeee@users.noreply.github.com> (https://deeplinknow.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deeplinknow/dln-react-native/issues"
  },
  "homepage": "https://github.com/deeplinknow/dln-react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.16.2",
    "expo-clipboard": "^7.0.1"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^9.0.2",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.0.3",
    "react": "^18.3.1",
    "react-native": "^0.76.7",
    "react-native-builder-bob": "^0.37.0",
    "release-it": "^18.1.2",
    "typescript": "~5.1.6"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    ".",
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "\\.d\\.ts$"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-bitwise": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.48.1"
  }
}
