{
  "name": "@deeplinknow/react-native",
  "version": "1.1.17",
  "description": "React Native + Expo SDK for the Deeplink Now deferred deep linking platform",
  "source": "./src/index.ts",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "test": "jest",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly && eslint \"**/*.{js,ts,tsx}\" --fix",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "typecheck": "tsc --noEmit",
    "build-plugin": "tsc --project plugin/tsconfig.json",
    "release": "pnpm clean && pnpm lint:fix && pnpm prepare && git add -A && git commit -m 'chore: prepare release' --no-verify && git push && pnpm release-it",
    "release-it": "release-it",
    "release:public": "bash scripts/release-public.sh"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "expo",
    "deep linking",
    "deferred deep linking",
    "attribution"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeplinknow/dln-react-native.git"
  },
  "author": "DeepLinkNow <2121331+jvgeee@users.noreply.github.com> (https://deeplinknow.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deeplinknow/dln-react-native/issues"
  },
  "homepage": "https://github.com/deeplinknow/dln-react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "expo": {
    "plugin": {
      "main": "./plugin/build/index.js",
      "types": "./plugin/build/index.d.ts"
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@deeplinknow/react-native"
        ]
      }
    }
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./app.plugin.js": "./app.plugin.js"
  },
  "files": [
    "src",
    "build",
    "android",
    "ios",
    "cpp",
    "plugin/build",
    "app.plugin.js",
    "*.podspec",
    "expo-module.config.json",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "dependencies": {
    "expo-modules-core": "~3.0.21"
  },
  "devDependencies": {
    "@expo/config-plugins": "~10.1.2",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^9.0.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.0.0",
    "@types/react": "~19.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "expo": "^54.0.18",
    "expo-module-scripts": "^5.0.7",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.0.3",
    "react": "*",
    "react-native": "0.81.5",
    "release-it": "^18.1.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "@expo/config-plugins": {
      "optional": true
    },
    "expo": {
      "optional": true
    }
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/build/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "\\.d\\.ts$"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "push": true
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-bitwise": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "build/",
    "plugin/build/",
    "app.plugin.js",
    "babel.config.js",
    "setupJest.js"
  ]
}
